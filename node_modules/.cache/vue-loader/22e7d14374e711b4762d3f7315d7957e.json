ple-drag .drop-active h3 {\n  margin: -.5em 0 0;\n  position: absolute;\n  top: 50%;\n  left: 0;\n  right: 0;\n  -webkit-transform: translateY(-50%);\n  -ms-transform: translateY(-50%);\n  transform: translateY(-50%);\n  font-size: 40px;\n  color: #fff;\n  padding: 0;\n}\n</style>\n\n<script>\nimport FileUpload from 'vue-upload-component'\nexport default {\n  components: {\n    FileUpload,\n  },\n\n  data() {\n    return {\n      files: [],\n\n      extensions: 'gpx',\n      // extensions: ['gif', 'jpg', 'jpeg','png', 'webp'],\n      // extensions: /\\.(gif|jpe?g|png|webp)$/i,\n      minSize: 1024,\n      multiple: false,\n      directory: false,\n      drop: true,\n      dropDirectory: true,\n      addIndex: false,\n      thread: 3,\n      name: 'file',\n      postAction: process.env.VUE_APP_APIGW_URL,\n      putAction: '/upload/put',\n      headers: {\n        //'X-Csrf-Token': 'xxxx',\n      },\n      data: {\n        //'_csrf_token': 'xxxxxx',\n      },\n    }\n  }\n}\n</script>\n"]}]}